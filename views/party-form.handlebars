<body>
  <main id="formContainer">
    <div class="container">
      <div class="row ">
        <form class="col s12">
          <div class="row">
            <div class="input-field col s12">
              <input placeholder="Placeholder" id="name" type="text" class="validate">
              <label for="name">Name</label>
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">
              <input placeholder="Placeholder" id="description" type="text" class="validate">
              <label for="description">Description</label>
            </div>
          </div>
