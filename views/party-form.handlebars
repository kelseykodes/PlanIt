            <div class="input-field col s6">
              <input placeholder="Placeholder" id="date" type="date" class="validate">
              <label for="date">Date</label>
            </div>
            <div class="input-field col s6">
              <input placeholder="Placeholder" id="number" type="number" class="validate">
              <label for="number">Number of Guests</label>
            </div>
